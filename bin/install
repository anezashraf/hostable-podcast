#!/usr/bin/env bash

yarn install --verbose
yarn build --verbose

composer install -vvv
php bin/console doctrine:database:create -vvv
php bin/console doctrine:migrations:migrate --no-interaction --query-time
php bin/console setting:create -vvv
php bin/console up -vvv
php bin/console user:create -vvv
php bin/console podcast:create -vvv